<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TabPane prefHeight="900.0" prefWidth="1440.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8"
         xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainController">
    <Tab fx:id="TabHome" onSelectionChanged="#refresh_scene" text="Home">
        <ScrollPane prefHeight="200.0" prefWidth="200.0">
            <FlowPane prefHeight="1072.0" prefWidth="1428.0">
                <padding>
                    <Insets bottom="50.0" left="50.0" right="50.0" top="50.0"/>
                </padding>
                <Pane prefHeight="171.0" prefWidth="1318.0">
                    <Label layoutX="14.0" layoutY="47.0" text="Play Now">
                        <font>
                            <Font size="60.0"/>
                        </font>
                    </Label>
                </Pane>
                <VBox prefHeight="200.0" prefWidth="100.0">
                    <FlowPane.margin>
                        <Insets bottom="50.0" left="50.0" right="50.0" top="50.0"/>
                    </FlowPane.margin>
                    <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true"
                               preserveRatio="true">
                        <VBox.margin>
                            <Insets/>
                        </VBox.margin>
                    </ImageView>
                    <Label text="Label"/>
                </VBox>
                <VBox prefHeight="200.0" prefWidth="100.0">
                    <FlowPane.margin>
                        <Insets bottom="50.0" left="50.0" right="50.0" top="50.0"/>
                    </FlowPane.margin>
                    <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true"
                               preserveRatio="true"/>
                    <Label text="Label"/>
                </VBox>
                <VBox prefHeight="200.0" prefWidth="100.0">
                    <FlowPane.margin>
                        <Insets bottom="50.0" left="50.0" right="50.0" top="50.0"/>
                    </FlowPane.margin>
                    <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true"
                               preserveRatio="true">
                        <VBox.margin>
                            <Insets/>
                        </VBox.margin>
                    </ImageView>
                    <Label text="Label"/>
                </VBox>
                <VBox prefHeight="200.0" prefWidth="100.0">
                    <FlowPane.margin>
                        <Insets bottom="50.0" left="50.0" right="50.0" top="50.0"/>
                    </FlowPane.margin>
                    <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true"
                               preserveRatio="true">
                        <VBox.margin>
                            <Insets/>
                        </VBox.margin>
                    </ImageView>
                    <Label text="Label"/>
                </VBox>
            </FlowPane>
        </ScrollPane>
    </Tab>
    <Tab fx:id="TabSearch" onSelectionChanged="#refresh_scene" text="Search">
        <GridPane alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <TextField fx:id="FieldSearch" GridPane.halignment="CENTER" GridPane.valignment="BOTTOM">
                <GridPane.margin>
                    <Insets bottom="50.0" left="200.0" right="200.0"/>
                </GridPane.margin>
                <font>
                    <Font size="24.0"/>
                </font>
            </TextField>
            <Button fx:id="ButtonSearch" alignment="TOP_LEFT" mnemonicParsing="false"
                    onAction="#on_button_search_clicked" text="Search" GridPane.halignment="CENTER"
                    GridPane.rowIndex="1" GridPane.valignment="TOP">
                <GridPane.margin>
                    <Insets top="50.0"/>
                </GridPane.margin>
                <font>
                    <Font size="96.0"/>
                </font>
            </Button>
        </GridPane>
    </Tab>
    <Tab fx:id="TabUser" onSelectionChanged="#refresh_scene" text="User">
        <SplitPane dividerPositions="0.25" prefHeight="160.0" prefWidth="200.0">
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
                <opaqueInsets>
                    <Insets/>
                </opaqueInsets>
                <GridPane alignment="CENTER" prefHeight="309.0" prefWidth="717.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints maxHeight="206.0" minHeight="10.0" prefHeight="161.0"
                                        vgrow="SOMETIMES"/>
                        <RowConstraints maxHeight="98.0" minHeight="0.0" prefHeight="45.0"
                                        vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true"
                               preserveRatio="true" GridPane.halignment="CENTER"
                               GridPane.valignment="CENTER">
                        <Image url="@avatar.png"/>
                    </ImageView>
                    <Label fx:id="LabelUsername" text="Label" GridPane.halignment="CENTER"
                           GridPane.rowIndex="1" GridPane.valignment="CENTER"/>
                    <Button fx:id="ButtonEditProfile" mnemonicParsing="false"
                            onAction="#on_button_edit_profile_clicked" text="Edit Profile"
                            GridPane.halignment="CENTER" GridPane.rowIndex="2"
                            GridPane.valignment="CENTER"/>
                </GridPane>
                <GridPane>
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <Label text="已玩桌游" GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
                    <Label text="积分" GridPane.halignment="CENTER" GridPane.rowIndex="1"
                           GridPane.valignment="CENTER"/>
                    <Label text="Label" GridPane.halignment="CENTER" GridPane.rowIndex="2"
                           GridPane.valignment="CENTER"/>
                </GridPane>
            </VBox>
            <ScrollPane prefHeight="200.0" prefWidth="200.0">
                <GridPane prefHeight="868.0" prefWidth="1055.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <Label text="Recently Played" GridPane.halignment="CENTER"
                           GridPane.valignment="CENTER">
                        <font>
                            <Font size="40.0"/>
                        </font>
                    </Label>
                    <GridPane GridPane.columnIndex="1" GridPane.rowIndex="1">
                        <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        </columnConstraints>
                        <rowConstraints>
                            <RowConstraints maxHeight="251.0" minHeight="10.0" prefHeight="246.0"
                                            vgrow="SOMETIMES"/>
                            <RowConstraints maxHeight="278.0" minHeight="10.0" prefHeight="43.0"
                                            vgrow="SOMETIMES"/>
                        </rowConstraints>
                        <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true"
                                   preserveRatio="true" GridPane.halignment="CENTER"
                                   GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER"
                                   GridPane.vgrow="ALWAYS"/>
                        <Label text="Label" GridPane.halignment="CENTER" GridPane.rowIndex="1"
                               GridPane.valignment="CENTER"/>
                    </GridPane>
                </GridPane>
            </ScrollPane>
        </SplitPane>
    </Tab>
    <Tab fx:id="TabSettings" onSelectionChanged="#refresh_scene" text="Settings">
        <GridPane alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <Label text="不知道放什么">
                <font>
                    <Font size="96.0"/>
                </font>
            </Label>
        </GridPane>
    </Tab>
</TabPane>
